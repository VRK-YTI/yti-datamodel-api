{
  "from": 1,
  "size": 100,
  "query": {
    "bool": {
      "must": [
        {
          "query_string": {
            "fields": [
              "label.*"
            ],
            "fuzziness": "2"
          }
        },
        {
          "terms": {
            "status": ["VALID", "SUGGESTED"]
          }
        },
        {
          "term": {
            "targetClass": {
              "value": "http://uri.suomi.fi/datamodel/ns/test/TestClass"
            }
          }
        },
        {
          "terms": {
            "resourceType": ["ATTRIBUTE", "ASSOCIATION"]
          }
        },
        {
          "bool": {
            "should": [
              {
                "terms": {
                  "isDefinedBy": [
                    "http://uri.suomi.fi/datamodel/ns/addedNs",
                    "http://external-data.com/test"
                  ]
                }
              },
              {
                "terms": {
                  "id": []
                }
              }
            ]
          }
        }
      ],
      "should": [
        {
          "bool": {
            "must_not": [
              {
                "term": {
                  "status": {
                    "value": "DRAFT"
                  }
                }
              }
            ]
          }
        },
        {
          "terms": {
            "isDefinedBy": ["http://uri.suomi.fi/datamodel/ns/test"]
          }
        }
      ]
    }
  },
  "sort": [
    {
      "label.en.keyword": {
        "order": "asc",
        "unmapped_type": "keyword"
      }
    }
  ]
}
